<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="advertisingspaceforrent.demo.data.QuestionMapper">

    <select id="selectQuestionByLanguageAndCategory" resultMap="Question">
        SELECT * FROM question WHERE language=#{getQuestionForm.language} and category=#{getQuestionForm.category}
    </select>

    <resultMap id="Question" type="advertisingspaceforrent.demo.po.Question"/>
</mapper>